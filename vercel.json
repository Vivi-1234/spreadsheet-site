{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/products",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/how-to-buy.html"
    }
  ]
}
